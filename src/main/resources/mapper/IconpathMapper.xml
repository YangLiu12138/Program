<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mydisney.mapper.IconpathMapper">

    <select id="getIndex" resultType="com.example.mydisney.entity.Iconpath">
        SELECT
            *
        FROM
            iconpath
        WHERE
            type = 'fensiqun'
           OR type = 'zuduiqun'
           OR type = 'news'
           OR type = 'gonglue'
    </select>
</mapper>
